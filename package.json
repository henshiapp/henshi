{
  "name": "henshi",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build",
    "dev": "nx run-many --target=dev --all --parallel",
    "lint": "nx run-many --target=lint --all --parallel",
    "format": "prettier --write \"**/*.{ts,vue,css}\"",
    "migrate": "pnpm -r run migration:run",
    "clean": "rm -rf `find . -type d -name dist -not -path \"./node_modules/*\"` && rm -rf .nx",
    "docker:dev:up": "docker compose up -d"
  },
  "private": true,
  "devDependencies": {
    "@nx/workspace": "^19.3.0",
    "@types/node": "~20.13.0",
    "eslint": "^9.4.0",
    "nx": "19.2.2",
    "prettier": "^3.3.0",
    "typescript": "~5.4.2"
  }
}